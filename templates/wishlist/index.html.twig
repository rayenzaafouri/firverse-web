<body data-context="shop">
{# data-context value should be one of the values "exercise","gym","events","shop","nutrition" or "user" depending on your module  #}
<h1>Wishlist index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Added_at</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for wishlist in wishlists %}
            <tr>
                <td>{{ wishlist.id }}</td>
                <td>{{ wishlist.addedAt ? wishlist.addedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_wishlist_show', {'id': wishlist.id}) }}">show</a>
                    <a href="{{ path('app_wishlist_edit', {'id': wishlist.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_wishlist_new') }}">Create new</a>
